{
  "kind": "collectionType",
  "collectionName": "items",
  "info": {
    "singularName": "item",
    "pluralName": "items",
    "displayName": "Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "Price": {
      "type": "decimal"
    },
    "Rating": {
      "type": "integer",
      "max": 5,
      "min": 1
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "items"
    },
    "isNew": {
      "type": "boolean",
      "default": false
    },
    "isSale": {
      "type": "boolean",
      "default": false
    }
  }
}
